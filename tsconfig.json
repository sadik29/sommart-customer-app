{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "typeRoots": ["./types", "./node_modules/@types"],
    "strict": true,
    "jsx": "react-native",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "types": ["react-native", "react-native-reanimated"],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "assets/Icons/index.js"
  ]
}
